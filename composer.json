{
    "name": "jonasarts/notification-bundle",
    "description": "An Email Notification System for Symfony",
    "keywords": ["notification", "email", "mail"],
    "homepage": "https://github.com/jonasarts/notification-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonas Hauser",
            "email": "symfony@jonasarts.com",
            "homepage": "http://www.jonasarts.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/config": "^6.0|^7.0",
        "symfony/dependency-injection": "^6.0|^7.0",
        "symfony/mailer": "^6.0|^7.0",
        "twig/twig": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "jonasarts\\Bundle\\NotificationBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "jonasarts\\Bundle\\NotificationBundle\\Tests\\": "tests/"
        }
    }
}
